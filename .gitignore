# ====================
# Frontend ignores
# ====================

# Ignore frontend dependencies and build outputs
/frontend/node_modules
/frontend/.next
/frontend/out
/frontend/build

# Ignore frontend environment files
/frontend/.env
/frontend/.env*.local

# Ignore misc files in frontend
/frontend/.DS_Store
/frontend/*.pem
/frontend/next-env.d.ts
/frontend/*.tsbuildinfo
/frontend/serviceAccountKey.json
/frontend/.vercel

# Testing
/frontend/coverage

# Logs
/frontend/npm-debug.log*
/frontend/yarn-debug.log*
/frontend/yarn-error.log*
/frontend/.pnpm-debug.log*

# PnP
/frontend/.pnp
/frontend/.pnp.js

# ====================
# Backend ignores
# ====================

# Node modules
/backend/node_modules

# Environment variables
/backend/.env
/backend/.env.local

# Logs
/backend/logs
/backend/*.log
/backend/npm-debug.log*
/backend/yarn-debug.log*
/backend/yarn-error.log*

# Build and dist
/backend/dist
/backend/build

# OS/system files
/backend/.DS_Store
/backend/Thumbs.db

# Editor folders
/backend/.vscode
/backend/.idea

# Temp files
/backend/*.tmp
/backend/*.bak
